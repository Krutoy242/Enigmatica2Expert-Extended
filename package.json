{
  "private": true,
  "type": "module",
  "files": [
    "./**/*js"
  ],
  "scripts": {
    "automate": "esno mc-tools/packages/run/src/cli.ts dev/tools/mct-run.json",
    "bansoukou-prune": "node \"dev/_other/advancement-fix.js\" -r",
    "build": "esno dev/make_pack.js",
    "dev": "concurrently npm:dev:* --prefix-colors auto",
    "dev:bansoukou": "esno dev/automation/bansoukou.js",
    "dev:benchmark": "mc-benchmark --output=dev/benchmarks/latest.md",
    "dev:conflicts": "esno dev/automation/recipe_conflicts.js",
    "dev:entities": "esno dev/automation/entities.js",
    "dev:errors": "esno mc-tools/packages/errors/src/cli.ts --config=dev/tools/mct-errors-config.yml --output=dev/tools/unresolved-errors.log",
    "dev:ftbq_cleanup": "esno mc-tools/scripts/ftbq_cleanup.ts",
    "dev:inject_js": "esno dev/automation/Inject_JS.js",
    "dev:jei": "esno dev/automation/jei.js",
    "dev:manifest": "esno mc-tools/packages/manifest/src/cli.ts --verbose --ignore=dev/.devonly.ignore --key=secrets/~cf_api_key.txt",
    "dev:misc": "esno dev/automation/misc.js --keep-cache",
    "dev:modlist": "esno mc-tools/packages/modlist/src/cli.ts --old=../E2E-unchanged/minecraftinstance.json --verbose --ignore=dev/.devonly.ignore --key=secrets/~cf_api_key.txt --template=dev/tools/modlist_template.md",
    "dev:patchouli": "esno dev/Patchouli/Patchouli.js",
    "dev:tcon": "esno mc-tools/packages/tcon/src/cli.ts --default=dev/default_configs/tweakersconstruct.cfg --tweaks=dev/tools/tcon/tweaks --save=dev/tools/tcon/stats",
    "dev:tellme": "esno config/tellme/!rename_update.js",
    "dev:tips": "esno dev/automation/tips.js",
    "dev:xml": "esno dev/automation/XML.js",
    "mc-tools:reducer": "esno mc-tools/packages/reducer/src/cli.ts levels dev/tools/reduce_mods.json"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "markdown-it": "^13.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@types/conventional-changelog-core": "^4.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/memoizee": "^0.4.8",
    "@types/terminal-kit": "^2.5.1",
    "adm-zip": "^0.5.10",
    "boxen": "^7.0.2",
    "chalk": "^5.2.0",
    "cli-progress": "^3.12.0",
    "compare-func": "^2.0.0",
    "concurrently": "^8.2.1",
    "csv-parse": "^5.3.8",
    "csvtojson": "^2.0.10",
    "curseforge-v2": "^1.2.0",
    "del": "^7.0.0",
    "detect-indent": "^7.0.1",
    "eslint": "^8.38.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fast-glob": "^3.2.12",
    "fast-levenshtein": "^3.0.0",
    "fs-extra": "^11.1.1",
    "git-describe": "^4.1.1",
    "glob": "^9.3.5",
    "glob-escape": "^0.0.2",
    "humanize-string": "^3.0.0",
    "ignore": "^5.2.4",
    "language-name-map": "^0.3.0",
    "lodash": "^4.17.21",
    "log-update": "^5.0.1",
    "mc-benchmark": "^1.0.5",
    "memoizee": "^0.4.15",
    "minimist": "^1.2.8",
    "nbt-ts": "^1.3.5",
    "node-curseforge": "^1.2.2",
    "node-fetch": "^3.3.1",
    "numeral": "^2.0.6",
    "open": "^9.1.0",
    "over": "^0.0.5",
    "parse-gitignore": "2.0.0",
    "pdf-parse": "^1.1.1",
    "replace-in-file": "^6.3.5",
    "simple-git": "^3.17.0",
    "snake-case": "^3.0.4",
    "ssh2-sftp-client": "^9.0.4",
    "table": "^6.8.1",
    "terminal-kit": "^3.0.0",
    "typescript": "^5.0.4",
    "xml-js": "^1.6.11",
    "yaml": "^2.2.2",
    "yargs": "^17.7.1",
    "zip-local": "^0.3.5"
  },
  "packageManager": "pnpm@8.2.0"
}
